<mapper namespace="com.example.coldchain.server.mapper.AdminMapper">

    <resultMap id="BaseResultMap" type="com.example.coldchain.server.pojo.Admin">
        <result property="id" column="id"/>
        <result property="name" column="name"/>
        <result property="password" column="password"/>
        <result property="teleNum" column="tele_num"/>
        <result property="company" column="company"/>
        <result property="department" column="department"/>
        <result property="email" column="email"/>
        <result property="kind" column="kind"/>
        <result property="delete" column="delete"/>
        <result property="createTime" column="create_time"/>
        <result property="updateTime" column="update_time"/>
    </resultMap>

    <sql id="Base_Column_List">
        id ,name ,password ,tele_num ,company ,department ,email ,kind ,delete ,create_time ,update_time
    </sql>

    <insert id="saveSelective"  useGeneratedKeys="true" keyProperty="id"  parameterType="com.example.coldchain.server.pojo.Admin">
        INSERT INTO admin
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="null!=id">
                id  ,
            </if>
            <if test="null!=name">
                name  ,
            </if>
            <if test="null!=password">
                password  ,
            </if>
            <if test="null!=teleNum">
                tele_num  ,
            </if>
            <if test="null!=company">
                company  ,
            </if>
            <if test="null!=department">
                department  ,
            </if>
            <if test="null!=email">
                email  ,
            </if>
            <if test="null!=kind">
                kind  ,
            </if>
            <if test="null!=delete">
                delete  ,
            </if>
            <if test="null!=createTime">
                create_time  ,
            </if>
            <if test="null!=updateTime">
                update_time
            </if>
        </trim>
        <trim prefix="VALUES (" suffix=")" suffixOverrides=",">
            <if test="null!=id">
                #{id, jdbcType=BIGINT} ,
            </if>
            <if test="null!=name">
                #{name, jdbcType=VARCHAR} ,
            </if>
            <if test="null!=password">
                #{password, jdbcType=VARCHAR} ,
            </if>
            <if test="null!=teleNum">
                #{teleNum, jdbcType=VARCHAR} ,
            </if>
            <if test="null!=company">
                #{company, jdbcType=VARCHAR} ,
            </if>
            <if test="null!=department">
                #{department, jdbcType=VARCHAR} ,
            </if>
            <if test="null!=email">
                #{email, jdbcType=VARCHAR} ,
            </if>
            <if test="null!=kind">
                #{kind, jdbcType=INTEGER} ,
            </if>
            <if test="null!=delete">
                #{delete, jdbcType=TINYINT} ,
            </if>
            <if test="null!=createTime">
                #{createTime, jdbcType=TIMESTAMP} ,
            </if>
            <if test="null!=updateTime">
                #{updateTime, jdbcType=TIMESTAMP}
            </if>
        </trim>
    </insert>

    <update id="updateByPrimaryKeySelective" parameterType="com.example.coldchain.server.pojo.Admin">
        UPDATE admin
        <set>
            <if test="null!=name and ''!=name ">
                name = #{name, jdbcType=VARCHAR} ,
            </if>
            <if test="null!=password and ''!=password ">
                password = #{password, jdbcType=VARCHAR} ,
            </if>
            <if test="null!=teleNum and ''!=teleNum ">
                tele_num = #{teleNum, jdbcType=VARCHAR} ,
            </if>
            <if test="null!=company and ''!=company ">
                company = #{company, jdbcType=VARCHAR} ,
            </if>
            <if test="null!=department and ''!=department ">
                department = #{department, jdbcType=VARCHAR} ,
            </if>
            <if test="null!=email and ''!=email ">
                email = #{email, jdbcType=VARCHAR} ,
            </if>
            <if test="null!=kind">
                kind = #{kind, jdbcType=INTEGER} ,
            </if>
            <if test="null!=delete">
                delete = #{delete, jdbcType=TINYINT} ,
            </if>
            <if test="null!=createTime">
                create_time = #{createTime, jdbcType=TIMESTAMP} ,
            </if>
            <if test="null!=updateTime">
                update_time = #{updateTime, jdbcType=TIMESTAMP}
            </if>
        </set>
        WHERE
        id = #{id}
    </update>

    <select id="getByPrimaryKey" parameterType="java.lang.Long" resultMap="BaseResultMap">
        SELECT
        <include refid="Base_Column_List" />
        FROM admin
        WHERE
        id = #{id}
    </select>

</mapper>
